{
  "packageManager": "yarn",
  "useConfigFiles": true,
  "router": false,
  "vuex": false,
  "typescript": true,
  "cssPreprocessor": "node-sass",
  "plugins": {
    "@vue/cli-plugin-babel": {},
    "@vue/cli-plugin-eslint": {
      "config": "standard",
      "lintOn": ["save", "commit"]
    },
    "@vue/cli-plugin-unit-jest": {}
  },
  "configs": {
    "eslintConfig": {
      "rules": {
        "no-alert": 0
      }
    }
  }
}

{
  "packageManager": "yarn",
  "useConfigFiles": true,
  "router": true,
  "vuex": true,
  "typescript": true,
  "cssPreprocessor": "node-sass",
  "plugins": {
    "@vue/cli-plugin-typescript": {
      "classComponent": true,
      "tslint": true,
      "lintOn": true,
      "useTsWithBabel": true
    },
    "@vue/cli-plugin-pwa": {},
    "@vue/cli-plugin-babel": {
      
    },
    "@vue/cli-plugin-eslint": {
      "config": "Prettier",
      "lintOn": ["save", "commit"]
    },
    "eslint-plugin-prettier": {},
    "@vue/eslint-config-prettier": {
      "prompts": false
    },
    "@vue/cli-plugin-unit-mocha": {},
    "@vue/cli-plugin-e2e-nightwatch": {}
  },
  "configs": {
    "eslintConfig": {
      "rules": {
        "no-alert": 0
      }
    }
  }
}
